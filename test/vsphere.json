{
	"timeout": 6000,
	"template-name": "DC0_H0_VM0",
	"linked": false,
	"ssh": {
		"user": "~",
		"ssh-private-key": "~/.ssh/id_rsa"
	},
	"cloud-init": {
		"package_update": false,
		"package_upgrade": false
	},
	"old-vm": "DC0_H0_VM0",
	"new-vm": {
		"name": "vm-autoscaled-test",
		"annotation": "autoscaled VM",
		"memory": 4096,
		"cpus": 4,
		"disk": 10240,
		"network": {
			"domain": "sample.com",
			"dns": {
				"search": [
					"sample.com"
				],
				"nameserver": [
					"8.8.8.8"
				]
			},
			"interfaces": [
				{
					"primary": false,
					"exists": true,
					"vnet": "vmnet4",
					"device": "vmxnet3",
					"type": "custom",
					"mac-address": "generate",
					"nic": "eth0",
					"dhcp": true,
					"use-dhcp-routes": true
				},
				{
					"primary": true,
					"exists": true,
					"vnet": "vmnet2",
					"device": "vmxnet3",
					"type": "custom",
					"mac-address": "generate",
					"nic": "eth1",
					"dhcp": false,
					"address": "192.168.1.26",
					"gateway": "192.168.1.1",
					"netmask": "255.255.255.0"
				}
			]
		}
	}
}